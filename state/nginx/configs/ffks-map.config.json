{
  "dataPath": "/data/",
  "siteName": "Freifunk Kassel",
  "mapSigmaScale": 0.5,
  "showContact": true,
  "maxAge": 14,
  "mapLayers": [
    { "name": "CartoDB",
      "url": "https://cartodb-basemaps-{s}.global.ssl.fastly.net/light_all/{z}/{x}/{y}.png",
      "config": {
        "subdomains": "1234",
        "type": "osm",
        "attribution": "<a href=\"http://cartodb.com/basemaps/\" target=\"_blank\">CartoDB</a>",
        "maxZoom": 18
      }
    },
    { "name": "Stamen.TonerLite",
      "url": "https://stamen-tiles-{s}.a.ssl.fastly.net/toner-lite/{z}/{x}/{y}.png",
      "config": {
        "subdomains": "abcd",
        "attribution": "Map tiles by <a href=\"http://stamen.com\">Stamen Design</a>, <a href=\"http://creativecommons.org/licenses/by/3.0\">CC BY 3.0</a> &mdash; Map data &copy; <a href=\"http://www.openstreetmap.org/copyright\">OpenStreetMap</a>",
        "minZoom": 0,
        "maxZoom": 20
      }
    },
    { "name": "MapQuest",
      "url": "https://otile{s}-s.mqcdn.com/tiles/1.0.0/osm/{z}/{x}/{y}.jpg",
      "config": {
        "subdomains": "1234",
        "type": "osm",
        "attribution": "Tiles &copy; <a href=\"https://www.mapquest.com/\" target=\"_blank\">MapQuest</a>, Data CC-BY-SA OpenStreetMap",
        "maxZoom": 18
      }
    }
  ]
}
